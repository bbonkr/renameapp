{
    "name": "rename",
    "peoductName": "Rename",
    "version": "1.4.4",
    "description": "Rename files.",
    "author": {
        "name": "Pon Cheol Ku",
        "email": "bbon@bbon.kr",
        "url": "https://bbon.kr"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbonkr/renameapp.git"
    },
    "bugs": {
        "url": "https://github.com/bbonkr/renameapp/issues",
        "email": "bbon@bbon.kr"
    },
    "main": "dist/main.js",
    "scripts": {
        "start": "npm run build:prod && cross-env NODE_ENV=production npm run electron",
        "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"wait-on http://localhost:3000 && electron ./dist/main.js\"",
        "build": "webpack",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "build:prod": "cross-env NODE_ENV=production npm run build",
        "electron": "electron ./dist/main.js",
        "dev:main": "webpack --config webpack.config.main.js --watch --mode=development",
        "dev:renderer": "webpack-dev-server --config webpack.config.renderer.dev.js --mode=development",
        "lint": "tsc --noEmit && eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
        "pack": "cross-env NODE_ENV=production electron-builder build",
        "clean": "npm run clean:dist && npm run clean:build && npm run clean:cache && npm run clean:packages",
        "clean:dist": "shx --silent rm -r ./dist",
        "clean:build": "shx rm -r ./.build",
        "clean:cache": "shx rm -r ./.cache",
        "clean:packages": "shx rm -r ./packages"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "autoprefixer": "^10.2.4",
        "notistack": "^1.0.3",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-transition-group": "^4.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.12.13",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
        "@types/node": "^14.14.22",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "asar": "^3.0.3",
        "babel-loader": "^8.2.2",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "electron": "^11.2.1",
        "electron-builder": "^22.9.1",
        "electron-packager": "^15.2.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.1",
        "prettier": "^2.2.1",
        "react-refresh": "^0.9.0",
        "shx": "^0.3.3",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.14",
        "typescript": "^4.1.3",
        "wait-on": "^5.2.1",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2"
    },
    "build": {
        "productName": "Rename",
        "appId": "kr.bbon.rename",
        "asar": true,
        "protocols": {
            "name": "Rename",
            "schemes": [
                "kr.bbon.Rename"
            ]
        },
        "files": [
            "dist/**/*"
        ],
        "mac": {
            "target": [
                "default"
            ],
            "icon": "./resources/installer/appicon.icns"
        },
        "dmg": {
            "title": "Rename App",
            "icon": "./resources/installer/appicon.icns"
        },
        "win": {
            "target": [
                "nsis",
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ],
            "icon": "./resources/installer/appicon.ico",
            "artifactName": "${productName}-installer-${os}-${arch}-${version}.${ext}"
        },
        "linux": {
            "target": [
                "AppImage",
                "deb",
                "rpm",
                "zip",
                "tar.gz"
            ],
            "icon": "./resources/installer/appicon.png"
        },
        "directories": {
            "buildResources": "resources/installer/",
            "output": "packages/",
            "app": "."
        }
    }
}
