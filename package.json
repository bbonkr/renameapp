{
    "name": "rename",
    "peoductName": "Rename",
    "version": "1.4.0",
    "description": "Rename files.",
    "main": "dist/main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "watch": "tsc --watch",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "build:prod": "cross-env NODE_ENV=production npm run build",
        "start": "npm run build && electron ./dist/main.js",
        "electron": "electron .",
        "lint": "tsc --noEmit && eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
        "pack:win-x64": "npm run build:prod && cross-env NODE_ENV=production electron-builder build --win --x64",
        "pack:win-x86": "npm run build:prod && cross-env NODE_ENV=production electron-builder build --win",
        "pack:win": "npm run build:prod && electron-builder build --win --x64 && electron-builder build --win",
        "pack:mac": "npm run build:prod && cross-env NODE_ENV=production electron-builder build --mac",
        "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && electron ./dist/main.js\"",
        "!!!dev:renderer": "does not work! parcel index.html --port 3000 --no-cache --out-dir .build --target electron",
        "!!!build:renderer": "does not work! parcel build index.html --no-cache --no-source-maps --out-dir .build --public-url ./ --target electron"
    },
    "author": "bbon (bbon@bbon.kr)",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^12.12.37",
        "@types/react-dom": "^16.9.7",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "asar": "^3.0.3",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0.2",
        "electron": "^8.2.5",
        "electron-builder": "^22.6.0",
        "electron-installer-dmg": "^3.0.0",
        "electron-packager": "^14.2.1",
        "electron-winstaller": "^4.0.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.0.0",
        "parcel": "^1.12.4",
        "prettier": "^2.0.5",
        "typescript": "^3.8.3",
        "wait-on": "^4.0.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.12",
        "@material-ui/icons": "^4.9.1",
        "autoprefixer": "^9.7.6",
        "electron-squirrel-startup": "^1.0.0",
        "notistack": "^0.9.11",
        "prop-types": "^15.6.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-transition-group": "^4.3.0"
    },
    "build": {
        "productName": "Rename",
        "appId": "kr.bbon.rename",
        "asar": true,
        "protocols": {
            "name": "Rename",
            "schemes": [
                "kr.bbon.Rename"
            ]
        },
        "files": [
            "index.html",
            "dist/**/*"
        ],
        "mac": {
            "target": [
                "default"
            ],
            "icon": "./resources/installer/appicon.icns"
        },
        "dmg": {
            "title": "Rename App",
            "icon": "./resources/installer/appicon.icns"
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ],
            "icon": "./resources/installer/appicon.ico",
            "artifactName": "${productName}-${os}-${arch}-${version}.${ext}"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "artifactName": "${productName} Setup ${os}-${arch}-${version}.${ext}"
        },
        "linux": {
            "target": [
                "AppImage",
                "deb",
                "rpm",
                "zip",
                "tar.gz"
            ],
            "icon": "./resources/installer/appicon.png"
        },
        "directories": {
            "buildResources": "resources/installer/",
            "output": "packages/",
            "app": "."
        }
    }
}
