{
    "name": "rename.app",
    "peoductName": "Rename App",
    "version": "1.0.0",
    "description": "Rename files.",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack --config webpack.config.js",
        "build:dev": "webpack --config webpack.config.js",
        "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "start": "electron .",
        "electron": "electron .",
        "pack": "electron-packager . Rename.App --out=pack --all=true --asar --overwrite",
        "pack:win": "electron-packager . Rename.App --out=pack --platform=win32 --arch=x64 --asar --overwrite",
        "pack:mac": "electron-packager . Rename.App --out=pack --platform=darwin --arch=x64 --asar --overwrite -m",
        "installer:win": "node make-win-installer.js",
        "installer:mac": "electron-installer-dmg ./pack/Rename.App-darwin-x64/Rename.App.app Rename.App --out=./pack/installer-mac --overwrite"
    },
    "author": "bbon (bbon@bbon.kr)",
    "license": "MIT",
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "asar": "^2.0.1",
        "babel-loader": "^8.0.5",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "electron": "^5.0.6",
        "electron-installer-dmg": "^3.0.0",
        "electron-packager": "^13.0.1",
        "electron-winstaller": "^2.7.0",
        "node-sass": "^4.12.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1"
    },
    "dependencies": {
        "autoprefixer": "^9.4.6",
        "bootstrap": "^4.2.1",
        "electron-squirrel-startup": "^1.0.0",
        "jquery": "^3.3.1",
        "popper.js": "^1.14.6",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-alert": "^4.0.4",
        "react-alert-template-basic": "^1.0.0",
        "react-dom": "^16.7.0",
        "react-transition-group": "^2.5.3"
    }
}
