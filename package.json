{
    "name": "rename.app",
    "peoductName": "Rename App",
    "version": "1.0.0",
    "description": "Rename files.",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "watch": "tsc --watch",
        "build:dev": "webpack --config webpack.config.js",
        "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "start": "npm run build && electron ./dist/main.js",
        "electron": "electron .",
        "pack": "electron-packager . Rename.App --out=pack --all=true --asar --overwrite",
        "pack:win": "electron-packager . Rename.App --out=pack --platform=win32 --arch=x64 --asar --overwrite",
        "pack:mac": "electron-packager . Rename.App --out=pack --platform=darwin --arch=x64 --asar --overwrite -m",
        "installer:win": "node make-win-installer.js",
        "installer:mac": "electron-installer-dmg ./pack/Rename.App-darwin-x64/Rename.App.app Rename.App --out=./pack/installer-mac --overwrite",
        "lint": "tslint -c tslint.json -p tsconfig.json"
    },
    "author": "bbon (bbon@bbon.kr)",
    "license": "MIT",
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "devDependencies": {
        "asar": "^2.0.1",
        "cross-env": "^6.0.3",
        "electron": "^7.1.1",
        "electron-installer-dmg": "^3.0.0",
        "electron-packager": "^14.1.0",
        "electron-winstaller": "^4.0.0",
        "prettier": "^1.19.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.6.0",
        "@material-ui/icons": "^4.5.1",
        "autoprefixer": "^9.7.1",
        "electron-squirrel-startup": "^1.0.0",
        "notistack": "^0.9.5",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-transition-group": "^4.3.0"
    }
}
